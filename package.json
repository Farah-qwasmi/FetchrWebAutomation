    "gulp": "^3.9.1",
    "gulp-brotli": "^1.2.1",
    "jest": "^26.6.3",
    "jest-cli": "^24.8.0",
    "webpack": "^5.26.2",
    "webpack-cli": "^4.5.0"
  }
}
